    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.2.0"
  },
  "engines": {
    "node": ">= 14.18.0",
    "npm": ">= 7.5.2"
  }
}
