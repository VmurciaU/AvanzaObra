{
  "name": "@backend/nodejs-avanzaobra",
  "version": "1.0.0",
  "description": "AvanzaObra Typescript Nodejs Express API server",
  "private": true,
  "author": {
    "name": "Gustavo Zuluaga",
    "email": "zuluaga.gustavo@correounivalle.edu.co"
  },
  "maintainers": [
    {
      "name": "Gustavo Zuluaga",
      "email": "zuluaga.gustavo@correounivalle.edu.co"
    }
  ],
  "license": "SEE LICENSE IN ./LICENSE",
  "scripts": {
    "transpile": "tsc",
    "clean": "rimraf dist",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.png src/**/*.ico src/**/*.js src/**/*.eot src/**/*.ttf src/**/*.pdf dist/src",
    "build": "run-s clean transpile copy-files",
    "watch": "tsc --watch",
    "start:docker": "node ./dist/src/index.js",
    "start": "yarn transpile & nodemon src/index.ts",
    "dev": "nodemon --inspect src/index.ts",
    "lint": "eslint 'src/**/*.{js,ts}' --ignore-pattern 'node_modules/'",
    "seed": "npx ts-node runSeed.ts"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "rest",
    "api",
    "server",
    "template"
  ],
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@hapi/boom": "^10.0.1",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.12",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.1.0",
    "festivos-colombianos": "^1.0.9",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "mssql": "^10.0.2",
    "mysql2": "^3.11.0",
    "nanoid": "^5.0.4",
    "ntp-time": "^2.0.3",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-ldapauth": "^3.0.1",
    "passport-local": "^1.0.0",
    "rimraf": "^5.0.5",
    "serve-favicon": "^2.5.0",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/ldapjs": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.10",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/serve-favicon": "^2.5.7",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/parser": "^8.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.0",
